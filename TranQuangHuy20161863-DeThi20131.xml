<?xml version="1.0" encoding="ISO-8859-1"?>

<xs:schema xmlns:xs="http://www.w3.org/">
  <xs:complexType>
		<xs:sequence>
			<xs:element name="food" maxOccurs="unbound">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="name" type="xs:string" />
						<xs:element name="price" type="xs:string" />
						<xs:element name="desciption" type="xs:string" minOccurs="0"/>
						<xs:element name="calories" type="xs:positiveInteger" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
  </xs:complexType>
</xs:schema>
